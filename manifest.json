{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension allows the user to change the background color of the current page.",
  "version": "1.0",

  "browser_action":
    {
      "default_icon":"icon.png",
      "default_popup":"popup.html"
    },

  "content_scripts":[
    {
    "matches":["<all_urls>"],
    "js":["hello.js"]
  }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "*://google.com/*",
    "tabs"
  ]
}
